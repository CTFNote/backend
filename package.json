{
  "name": "ctfnote-backend",
  "version": "0.1.0-alpha",
  "description": " The backend for CTFNote, made using Node.js, express.js, and Typescript.",
  "main": "src/app.ts",
  "repository": "git@github.com:CTFNote/backend",
  "author": "ent3r_",
  "license": "GPL-3.0-or-later",
