    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongoose": "5.10.2",
    "@types/morgan": "1.9.2",
    "@types/nodemon": "1.19.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.17.0",
    "husky": "4.3.7",
    "nodemon": "2.0.6",
    "standard-version": "9.1.0"
